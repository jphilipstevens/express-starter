{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Starter kit for building an express/node app using babel for ES6 and await/async support.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robzhu/express-starter.git"
  },
  "author": "robzhu",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/robzhu/express-starter/issues"
  },
  "homepage": "https://github.com/robzhu/express-starter#readme",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator",
      "transform-runtime"
    ]
  },
  "scripts": {
    "build": "babel src -d build",
    "start": "node build/index.js",
    "dev": "babel-watch src/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-watch": "^2.0.2"
  },
  "dependencies": {
    "express": "^4.14.0"
  }
}
